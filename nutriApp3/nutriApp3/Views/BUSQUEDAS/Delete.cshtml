@model nutriApp3.Models.BUSQUEDAS

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>BUSQUEDAS</legend>

    <div class="display-label">ID_USUARIO</div>
    <div class="display-field">@Model.ID_USUARIO</div>

    <div class="display-label">FECHA_HORA</div>
    <div class="display-field">@String.Format("{0:g}", Model.FECHA_HORA)</div>

    <div class="display-label">USUARIOS</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.USUARIOS).ToString()</div>

    <div class="display-label">COMERCIOS</div>
    <div class="display-field">@(Model.COMERCIOS == null ? "None" : Model.COMERCIOS.Count.ToString())</div>

    <div class="display-label">PRODUCTOS</div>
    <div class="display-field">@(Model.PRODUCTOS == null ? "None" : Model.PRODUCTOS.Count.ToString())</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


