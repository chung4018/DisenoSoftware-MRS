@model NutriApp5.Models.BUSQUEDAS

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>BUSQUEDAS</legend>

    <div class="display-label">ID_USUARIO</div>
    <div class="display-field">@Model.ID_USUARIO</div>

    <div class="display-label">FECHA_HORA</div>
    <div class="display-field">@String.Format("{0:g}", Model.FECHA_HORA)</div>

    <div class="display-label">USUARIOS</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.USUARIOS).ToString()</div>

    <div class="display-label">BUSQUEDAXCOMERCIOS</div>
    <div class="display-field">@(Model.BUSQUEDAXCOMERCIOS == null ? "None" : Model.BUSQUEDAXCOMERCIOS.Count.ToString())</div>

    <div class="display-label">BUSQUEDAXPRODUCTOS</div>
    <div class="display-field">@(Model.BUSQUEDAXPRODUCTOS == null ? "None" : Model.BUSQUEDAXPRODUCTOS.Count.ToString())</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID_BUSQUEDA }) |
    @Html.ActionLink("Back to List", "Index")
</p>


