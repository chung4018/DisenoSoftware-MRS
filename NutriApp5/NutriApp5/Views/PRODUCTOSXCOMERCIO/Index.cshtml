@model IEnumerable<NutriApp5.Models.PRODUCTOSXCOMERCIO>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            ID_COMERCIO
        </th>
        <th>
            ID_PRODUCTO
        </th>
        <th>
            COMERCIOS
        </th>
        <th>
            PRODUCTOS
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
        <td>
			@item.ID_COMERCIO
        </td>
        <td>
			@item.ID_PRODUCTO
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.COMERCIOS).ToString()
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.PRODUCTOS).ToString()
        </td>
    </tr>
}

</table>


