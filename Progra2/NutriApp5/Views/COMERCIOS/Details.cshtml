@model NutriApp5.Models.COMERCIOS

@{
    ViewBag.Title = "Detalles del comercio...";
}

<h2>Detalles</h2>

<fieldset>
    <legend>COMERCIOS</legend>

   <!-- <div class="display-label">ID_TIPO</div>
    <div class="display-field">@String.Format("{0:F}", Model.ID_TIPO)</div>

    <div class="display-label">ID_USUARIO</div>
    <div class="display-field">@Model.ID_USUARIO</div>-->

    <div class="display-label">NOMBRE</div>
    <div class="display-field">@Model.NOMBRE</div>

    <div class="display-label">LATITUD</div>
    <div class="display-field">@String.Format("{0:F}", Model.LATITUD)</div>

    <div class="display-label">LONGUITUD</div>
    <div class="display-field">@String.Format("{0:F}", Model.LONGUITUD)</div>

    <div class="display-label">TELEFONO</div>
    <div class="display-field">@String.Format("{0:F}", Model.TELEFONO)</div>

    <div class="display-label">CORREO</div>
    <div class="display-field">@Model.CORREO</div>

    <div class="display-label">BUSQUEDAXCOMERCIOS</div>
    <div class="display-field">@(Model.BUSQUEDAXCOMERCIOS == null ? "None" : Model.BUSQUEDAXCOMERCIOS.Count.ToString())</div>

    <div class="display-label">PRODUCTOSXCOMERCIO</div>
    <div class="display-field">@(Model.PRODUCTOSXCOMERCIO == null ? "None" : Model.PRODUCTOSXCOMERCIO.Count.ToString())</div>

    <div class="display-label">TIPO_COMERCIO</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.TIPO_COMERCIO).ToString()</div>
</fieldset>
<p>
    @Html.ActionLink("Editar", "Edit", new { id=Model.ID_COMERCIO }) |
    @Html.ActionLink("Atrás", "Index")
</p>


