@model NutriApp5.Models.USUARIOXCONDICION

@{
    ViewBag.Title = "Detalles";
}

<h2>Detalle de los usuarios</h2>

<fieldset>
    <legend>USUARIOXCONDICION</legend>

    <div class="display-label">ID_USUARIO</div>
    <div class="display-field">@Model.ID_USUARIO</div>

    <div class="display-label">ID_CONDICION</div>
    <div class="display-field">@String.Format("{0:F}", Model.ID_CONDICION)</div>

    <div class="display-label">CONDICION</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.CONDICION).ToString()</div>

    <div class="display-label">USUARIOS</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.USUARIOS).ToString()</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>


